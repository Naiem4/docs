    "schemas": {
      "BusinessInfo": {
        "type": "object",
        "properties": {
          "businessName": {
            "type": "string",
            "description": "Name of the business shown publicly.",
            "example": "Willro"
          },
          "website": {
            "type": "string",
            "format": "uri",
            "description": "URL of the business website.",
            "example": "https://willro.com"
          },
          "category": {
            "type": "string",
            "description": "Business category describing industry or sector.",
            "example": "Tech"
          },
          "averageRating": {
            "type": "number",
            "description": "Average user rating of the business on the platform.",
            "example": 4
          },
          "totalReview": {
            "type": "integer",
            "description": "Total number of user reviews for the business.",
            "example": 5
          },
          "googleMapUrl": {
            "type": "string",
            "format": "uri",
            "description": "Link to the business's location on Google Maps.",
            "example": "https://www.google.com/maps"
          },
          "location": {
            "type": "string",
            "description": "Business physical location or address. May be null if not available.",
            "example": "Landon, UK"
          }
        },
        "example": {
          "businessName": "Willro",
          "website": "https://willro.com",
          "category": "Tech",
          "averageRating": 4,
          "totalReview": 5,
          "googleMapUrl": "https://www.google.com/maps",
          "location": "Landon, UK"
        }
      },
